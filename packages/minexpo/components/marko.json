{
  "taglib-imports": [
    "./blocks/marko.json",
    "./elements/marko.json",
    "./flows/marko.json",
    "./leaders/marko.json",
    "./nodes/marko.json",
    "./page-wrappers/marko.json"
  ],
  "<minexpo-site-footer>": {
    "template": "./site-footer.marko"
  },
  "<directory-facets>": {
    "template": "./facets.marko",
    "@facets": "array",
    "@content-type": "string",
    "@search-query": "string",
    "@aliases": "array",
    "@active-id": "expression",
    "@title": "string",
    "@description": "string",
    "@with-toggle": {
      "type": "boolean",
      "default-value": false
    },
    "@initially-expanded": {
      "type": "boolean",
      "default-value": false
    }
  }
}
