{
  "<shared-inquiry-block>": {
    "template": "./inquiry.marko",
    "@content": "object",
    "@modifiers": "array",
    "@with-header": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<shared-directory-categories-block>": {
    "template": "./directory-categories.marko",
    "@aliases": "array",
    "@active-id": "number",
    "@content-type": "string",
    "@search-query": "string",
    "@primary-alias": "string",
    "@title": "string",
    "@description": "string",
    "@with-toggle": {
      "type": "boolean",
      "default-value": false
    },
    "@initially-expanded": {
      "type": "boolean",
      "default-value": false
    }
  },
  "<shared-company-product-categories-block>": {
    "template": "./company-product-categories.marko",
    "@alias": "array",
    "@schedules": "array"
  },
  "<shared-company-export-categories-block>": {
    "template": "./company-export-categories.marko",
    "@alias": "array",
    "@schedules": "array"
  },
  "<shared-content-load-more-block>": {
    "template": "./content-load-more.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@adIndex" : "number",
    "@adName" : "string",
    "@id": "number",
    "@type": "string",
    "@name": "string",
    "@aliases": "array",
    "@section-id": "number",
    "@section-name": "string",
    "@section-skip": "number"
  },
  "<common-featured-companies>": {
    "template": "./featured-companies.marko",
    "@content-id": {
      "type": "number",
      "required": true
    }
  },
  "<common-company-details>": {
    "template": "./company-details.marko",
    "@company": "object"
  },
  "<common-leadership-company-videos>": {
    "template": "./leadership-company-videos.marko",
    "@content-id": {
      "type": "number",
      "required": true
    }
  }
}
