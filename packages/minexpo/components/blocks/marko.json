{
  "<shared-inquiry-block>": {
    "template": "./inquiry.marko",
    "@content": "object",
    "@modifiers": "array",
    "@with-header": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<shared-directory-categories-block>": {
    "template": "./directory-categories.marko",
    "@aliases": "array",
    "@active-id": "number",
    "@content-type": "string",
    "@phrase": "string",
    "@primary-alias": "string",
    "@title": "string",
    "@description": "string",
    "@with-toggle": {
      "type": "boolean",
      "default-value": false
    }
  },
  "<minexpo-company-product-categories-block>": {
    "template": "./company-product-categories.marko",
    "@alias": "array",
    "@schedules": "array"
  },
  "<minexpo-content-load-more-block>": {
    "template": "./content-load-more.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@adIndex" : "number",
    "@adName" : "string",
    "@id": "number",
    "@type": "string",
    "@name": "string",
    "@aliases": "array",
    "@section-id": "number",
    "@section-name": "string",
    "@section-skip": "number"
  }
}
