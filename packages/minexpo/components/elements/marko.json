{
  "tags": {
    "shared-gam-content-targeting": {
      "template": "./gam-content-targeting.marko",
      "@obj": "object"
    },
    "shared-company-contact-details": {
      "template": "./company-contact-details.marko",
      "@obj": "object",
      "@show-email": {
        "type": "boolean",
        "default-value": true
      },
      "@tag": {
        "type": "string",
        "default-value": "div"
      },
      "@class": "string",
      "@modifiers": "array",
      "@attrs": "object",
      "@website-link-attrs": "object",
      "@social-link-attrs": "object"
    }
  },
  "<shared-company-logo>": {
    "template": "./company-logo.marko",
    "@content": "object"
  },
  "<shared-image-slider>": {
    "template": "./image-slider.marko",
    "@images" : "array"
  },
  "<shared-supplier-disclaimer>": {
    "template": "./supplier-disclaimer.marko",
    "@obj": "object"
  }
}
